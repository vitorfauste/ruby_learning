{
"files.associations": {
  "*.erb": "erb"
},
"[erb]": {
  "editor.defaultFormatter":"aliariff.vscode-erb-beautify"
},
"[html]": {
  "editor.defaultFormatter": "aliariff.vscode-erb-beautify"
},

"emmet.includeLanguages": {
  "erb": "html"
},
"emmet.showAbbreviationSuggestions": true,
"emmet.showSuggestionsAsSnippets": true
}